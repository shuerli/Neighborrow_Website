<h1>Sign Up</h1>
<%= form_for :account, url: accounts_path do |f| %>
    <p>
        <%= f.label :email %><br>
        <%= f.text_field :title %><br>
    </p>

    <p>
        <%= f.label :password %><br>
        <%= f.text_field :body %>
    </p>

    <p>
        <%= f.submit %>
    </p>
<% end %>
